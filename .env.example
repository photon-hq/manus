# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/manus_imessage
DB_PASSWORD=password

# Redis
REDIS_URL=redis://localhost:6379

# iMessage Integration (advanced-imessage-kit)
# Your Photon iMessage server endpoint and API key
IMESSAGE_SERVER_URL=https://your-imessage-server.photon.codes
IMESSAGE_API_KEY=your_photon_imessage_api_key

# Manus API Configuration
# Base URL for Manus AI API (defaults to production if not set)
MANUS_API_URL=https://api.manus.ai

# LLM Provider
OPENROUTER_API_KEY=your_openrouter_key

# SigNoz
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318

# App Config
PORT=3000
NODE_ENV=development
# Public URL for webhooks - your deployed backend URL that Manus will use
# For local: http://localhost:3000, For production: https://manus.yourdomain.com
PUBLIC_URL=http://localhost:3000

# Photon Configuration
PHOTON_NUMBER=+1234567890
# API key for MCP server authentication (generated during connection setup)
PHOTON_API_KEY=photon_sk_your_generated_key
